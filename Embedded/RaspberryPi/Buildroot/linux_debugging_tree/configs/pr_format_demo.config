# Enable the custom kernel module package
BR2_PACKAGE_PR_FORMAT_DEMO_KERNEL_MODULE=y

# Specify additional kernel configuration options (if needed)
# Enable the kernel module for your target system
BR2_LINUX_KERNEL=y
#BR2_LINUX_KERNEL_USE_CUSTOM_CONFIG=y
#BR2_LINUX_KERNEL_CUSTOM_CONFIG_FILE="path/to/your_kernel_config"

# Enable kernel module support (if it's a loadable module)
BR2_PACKAGE_KERNEL_MODULES=y
